{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "3.8.4",
    "axios": "0.18.0",
    "core-js": "2.6.2",
    "moment": "2.24.0",
    "node-sass": "4.11.0",
    "promise": "8.0.2",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "raf": "3.4.1",
    "react": "16.7.0",
    "react-ckeditor-component": "1.1.0",
    "react-document-title": "2.0.3",
    "react-dom": "16.7.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.1",
    "store": "2.0.12",
    "uuid": "3.3.2"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.7028cf81.chunk.js",
    "start": "react-app-rewired start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build",
    "test": "react-scripts test --reporters=default --reporters=jest-html-reporters --collectCoverage",
    "eject": "react-app-rewired eject",
    "ci": "react-scripts test --coverage --coverageDirectory=ci/coverage --coverageReporters=cobertura"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "**/*.{css,scss}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.15.0",
    "babel-plugin-import": "1.11.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-preset-env": "1.7.0",
    "compression-webpack-plugin": "3.0.0",
    "cross-env": "5.2.0",
    "css-split-webpack-plugin": "0.2.6",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.5",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-prettier": "3.0.0",
    "http-proxy-middleware": "0.19.1",
    "husky": "1.2.0",
    "jest-html-reporters": "1.1.5",
    "lint-staged": "8.1.0",
    "prettier": "1.15.3",
    "prettier-stylelint": "0.4.2",
    "pretty-quick": "1.8.0",
    "react-app-rewired": "1.6.2",
    "sinon": "7.1.1",
    "source-map-explorer": "1.6.0",
    "stylelint-config-standard": "18.2.0",
    "stylelint-prettier": "1.0.5",
    "stylelint-scss": "3.4.1",
    "sanitize-html": "1.20.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js}",
      "!src/index.js",
      "!src/utils/polyfill.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 9",
    "not op_mini all"
  ]
}